    <TABLE NAME="quizaccess_mproct_auth" COMMENT="Stores the user attempt level M Proctoring Auth .">
     <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="attemptid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Foreign key to quiz_attempts id."/>
        <FIELD NAME="quizid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Foreign key to quiz id."/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Foreign key to user id."/>
        <FIELD NAME="auth_face_id" TYPE="text" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Azure face id."/>
        <FIELD NAME="facepath" TYPE="text" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="User screen capture files"/>
        <FIELD NAME="idpath" TYPE="text" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Size of record user url."/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
